	@ if test -d $(srcdir)/$(HTML_DIR); then			\
	   thisdir=`pwd`;						\
	   docdir=`basename $$thisdir`;					\
	   $(mkinstalldirs) $(DOC_DIR);					\
	   cmd="rm -rf $(DOC_DIR)/$$docdir";				\
	   echo $$cmd;							\
	   eval $$cmd;							\
	   cmd="cp -r $(srcdir)/$(HTML_DIR) $(DOC_DIR)/$$docdir";	\
	   echo $$cmd;							\
	   eval $$cmd;							\
	fi

