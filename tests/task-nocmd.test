#! /bin/sh

$BUILDDIR/src/ifm -I$SRCDIR/lib -w -task -format raw 2>&1 > $BUILDDIR/tests/task-nocmd.out <<END
# Test of tasks with no commands.

room "Start";
task "Do first thing";

room "Finish" dir s;
task "Visit finish room" after last cmd none;
task "Do second thing" after last;
END

cmp -s $SRCDIR/tests/task-nocmd.exp $BUILDDIR/tests/task-nocmd.out
