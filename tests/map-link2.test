#! /bin/sh

$BUILDDIR/src/ifm -I$SRCDIR/lib -w -map -format raw 2>&1 > $BUILDDIR/tests/map-link2.out <<END
# Test of multiple links.

room "North Room" tag NR exit se sw;
room "South Room" tag SR dir s exit e w;

link SR to NR dir e nw;
link SR to NR dir w ne;
END

cmp -s $SRCDIR/tests/map-link2.exp $BUILDDIR/tests/map-link2.out
